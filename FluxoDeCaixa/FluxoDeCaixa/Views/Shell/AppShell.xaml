<?xml version="1.0" encoding="UTF-8" ?>
<Shell
	x:Class="FluxoDeCaixa.Mobile.AppShell"
	xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	xmlns:views="clr-namespace:FluxoDeCaixa.Mobile.Views"
	xmlns:local="clr-namespace:FluxoDeCaixa.Mobile">

	<TabBar>
		<ShellContent
			Title="Main"
			Icon="iconblank.png"
			ContentTemplate="{DataTemplate views:MainPage}" />
		<ShellContent
			Title="Settings"
			Icon="iconblank.png"
			ContentTemplate="{DataTemplate views:SettingsPage}" />
	</TabBar>
</Shell>
